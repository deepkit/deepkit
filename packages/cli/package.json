{
  "name": "@deepkit/cli",
  "description": "Command line application for Deepkit app",
  "version": "0.0.5",
  "author": "Deepkit (limited) <info@deepkit.ai>",
  "bin": "main.js",
  "private": true,
  "sideEffects": false,
  "bugs": "https://gitlab.com/deepkit/deepkit-cli/deepkit-cli/issues",
  "dependencies": {
    "@oclif/plugin-help": "^2.1.6",
    "cli-ux": "^3.3.30"
  },
  "devDependencies": {
    "@deepkit/core": "^0.0.5",
    "@deepkit/core-node": "^0.0.5",
    "@marcj/estdlib": "^0.1.17",
    "@marcj/estdlib-rxjs": "^0.1.13",
    "@marcj/glut-client": "^0.0.33",
    "@marcj/glut-core": "^0.0.33",
    "@marcj/marshal": "^2.1.3",
    "@types/dockerode": "^2.5.4",
    "@types/dot-prop": "^4.2.0",
    "@types/execa": "^2.0.0",
    "@types/fs-extra": "^5.0.2",
    "@types/getos": "^3.0.0",
    "@types/jest": "^23.3.9",
    "@oclif/command": "^1.5.13",
    "@oclif/config": "^1.12.12",
    "@types/node": "^10",
    "@types/ws": "^6.0.1",
    "@types/cli-progress": "^3.4.2",
    "cli-progress": "^3.4.0",
    "strip-ansi": "^5.2.0",
    "systeminformation": "^4.21.1",
    "chalk": "^3.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "chai": "^4",
    "clone": "^1.0.4",
    "dockerode": "^2.5.5",
    "events": "^2.0.0",
    "execa": "^2.0.4",
    "fast-glob": "^2.2.2",
    "fast-json-patch": "^2.0.6",
    "fast-xml-parser": "^3.12.20",
    "fs-extra": "^6.0.1",
    "get-parameter-names": "^0.3.0",
    "getos": "^3.1.1",
    "globby": "^8.0.1",
    "inspectpack": "^4.2.1",
    "isomorphic-git": "^0.54.2",
    "jest": "^23.6.0",
    "jest-extended": "^0.11.0",
    "js-yaml": "^3.11.0",
    "md5-file": "^4.0.0",
    "native-ext-loader": "^2.3.0",
    "nyc": "^13",
    "reflect-metadata": "^0.1.12",
    "relative": "^3.0.2",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "rxjs": "~6.5.3",
    "source-map-support": "^0.5.9",
    "tmp": "^0.0.33",
    "ts-jest": "22.0.4",
    "ts-node": "^8.1.0",
    "tslib": "^1.9.3",
    "typescript": "^3.3.4000",
    "uuid": "^3.2.1",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.1",
    "webpack-node-externals": "^1.7.2",
    "webpack-visualizer-plugin": "^0.1.11",
    "xbytes": "^1.5.0-0",
    "ws": "^7.2.1",
    "yaml": "^1.0.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": ""
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "keywords": [
    "deepkit"
  ],
  "main": "lib/index.js",
  "license": "closed",
  "oclif": {
    "bin": "deepkit",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "macos": {
      "identifier": "ai.deepkit.cli"
    }
  },
  "scripts": {
    "__build-controls": "node_modules/.bin/webpack --progress --config webpack.control-commands.config.js",
    "build": "node_modules/.bin/webpack --config webpack.config.js",
    "__build-binary": "nexe --build --input dist/main.js --resource dist/diskusage.node -o dist/main",
    "__postpack": "rm -f .oclif.manifest.json",
    "__prepack": "rm -rf lib && tsc && NODE_PRESERVE_SYMLINKS=1 oclif-dev manifest && NODE_PRESERVE_SYMLINKS=1 oclif-dev readme",
    "test": "echo NO TESTS",
    "__version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/*.spec.ts"
    ],
    "setupTestFrameworkScriptFile": "jest-extended",
    "globals": {
      "ts-jest": {
        "tsConfig": "tests/tsconfig.json"
      }
    }
  }
}
